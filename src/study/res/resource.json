{
	"fontSet": "Inconsolata, \"Hiragino Sans\", \"BIZ UDGothic\", \"Yu Gothic Medium\", Meiryo, monospace",
	"defaultExt": ".js",
	"jsHintOpt": {
		"bitwise": true,
		"curly": true,
		"eqeqeq": true,
		"esversion": 10,
		"latedef": false,
		"strict": false,
		"undef": false,
		"unused": false,
		"varstmt": true,
		"expr": false,
		"plusplus": true,
		"browser": true
	},
	"jsBeautifyOpt": {
		"preserve_newlines": true,
		"jslint_happy": false,
		"brace_style": "collapse",
		"keep_array_indentation": false,
		"space_before_conditional": true,
		"unescape_strings": true,
		"space_after_anon_function": true
	},
