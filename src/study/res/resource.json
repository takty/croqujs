{
    "fontSet": "Inconsolata, \"Hiragino Kaku Gothic ProN\", Meiryo, \"Yu Gothic\", YuGothic, monospace",
    "defaultExt": ".js",
    "jsHintOpt": {
        "bitwise": true,
        "curly": true,
        "eqeqeq": true,
        "latedef": false,
        "noarg": true,
        "plusplus": true,
        "undef": false,
        "strict": false,
        "esversion": 6,
        "expr": false,
        "browser": true
    },
    "jsBeautifyOpt": {
        "preserve_newlines": true,
        "jslint_happy": false,
        "brace_style": "collapse",
        "keep_array_indentation": false,
        "space_before_conditional": true,
        "unescape_strings": true,
        "space_after_anon_function": true
    },
